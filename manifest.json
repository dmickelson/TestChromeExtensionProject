{
  "manifest_version": 3,
  "name": "Hello Extensions of the world!",
  "description": "Base Level Extension",
  "version": "1.0",
  "icons": {
   "16": "images/icon-16.png",
   "48": "images/icon-48.png",
   "128": "images/icon-128.png"
 },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "images/hello_extensions.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
